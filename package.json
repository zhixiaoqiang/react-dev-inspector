{
  "name": "react-dev-inspector",
  "version": "1.6.0",
  "sideEffects": false,
  "description": "dev-tool for inspect react components and jump to local IDE for component code.",
  "workspaces": [
    ".",
    "./sites/*"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "test": "jest tests",
    "clean": "rimraf build dist site es lib plugins",
    "dev": "nodemon -w src -e ts,tsx -x scripts/dev.sh",
    "site": "scripts/dev-site.sh",
    "build": "scripts/build-package.sh",
    "build:site": "scripts/build-sites.sh",
    "vercel-build": "scripts/vercel-build.sh",
    "tag": "git tag v$(node -e 'console.log(require(\"./package.json\").version)')",
    "lint": "TIMING=1 eslint --ext js,jsx,ts,tsx src sites"
  },
  "repository": "zthxxx/react-dev-inspector",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "react",
    "component",
    "inspector"
  ],
  "author": "zthxxx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zthxxx/react-dev-inspector/issues"
  },
  "homepage": "https://github.com/zthxxx/react-dev-inspector#readme",
  "files": [
    "README.md",
    ".npmrc",
    "scripts",
    "src",
    "es",
    "lib",
    "plugins"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/generator": "^7.11.6",
    "@babel/parser": "^7.11.5",
    "@babel/traverse": "^7.11.5",
    "hotkeys-js": "^3.8.1",
    "loader-utils": "^2.0.0",
    "querystring": "^0.2.1",
    "react-dev-utils": "^11.0.0"
  },
  "devDependencies": {
    "@babel/types": "7.15.6",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/babel__traverse": "7.14.2",
    "@types/connect": "3.4.35",
    "@types/express": "4.17.13",
    "@types/fs-extra": "9.0.12",
    "@types/react": "17.0.22",
    "@types/react-reconciler": "0.26.3",
    "@types/webpack": "4.41.31",
    "@types/webpack-dev-server": "3.11.6",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "@umijs/types": "3.5.19",
    "babel-plugin-tester": "10.1.0",
    "babel-preset-react-app": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-alloy": "3.10.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-typescript": "0.14.0",
    "fs-extra": "10.0.0",
    "globby": "11.0.4",
    "husky": "4.3.8",
    "jest": "27.2.4",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "react": "16.14.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "vite": "2.5.10",
    "webpack": "4.46.0",
    "webpack-chain": "6.5.1"
  },
  "peerDependencies": {
    "@umijs/types": "^3.2.20",
    "react": ">=16.8.0",
    "webpack": ">=4.44.1",
    "webpack-chain": "^6.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ext js,jsx,ts,tsx"
  },
  "renovate": {
    "enabled": false
  }
}
